%h1 Listing Owners #{ link_to "(+)", new_owner_url}

%table
    %tr
        %th Name
        %th Id
        %th Key
        %th Action        
    - @owners.each do |owner|
        %tr
            %td= link_to owner.displayName, owner
            %td= owner.id   
            %td= owner.key
            %td
                = link_to 'delete', owner, :method => "delete"
