%table
    %tr
        %th Active
        %th ID
        %th Product Name        
        %th Quantity
        %th Consumed
        %th Start Date
        %th End Date        
    - pools.each do |pool|
        %tr
            %td= pool.activeSubscription
            %td= link_to pool.Id, pool if pool.Id
            %td= link_to pool.productName, product_url(:id =>   pool.productId)
            %td= pool.quantity
            %td= pool.consumed            
            %td= pool.startDate
            %td= pool.endDate